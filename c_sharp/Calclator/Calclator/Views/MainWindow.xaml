<Window x:Class="Calclator.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}" Height="350" Width="525" WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="53,65,0,0" TextWrapping="Wrap" Text="{Binding X}" VerticalAlignment="Top" Width="103"/>
        <TextBox HorizontalAlignment="Left" Height="28" Margin="53,117,0,0" TextWrapping="Wrap" Text="{Binding Y}" VerticalAlignment="Top" Width="103"/>
        <TextBox HorizontalAlignment="Left" Height="36" Margin="53,169,0,0" TextWrapping="Wrap" Text="{Binding Ans}" VerticalAlignment="Top" Width="103"/>
        <Button Content="Button" Command="{Binding Path=calcCommand}" HorizontalAlignment="Left" Height="36" Margin="236,117,0,0" VerticalAlignment="Top" Width="88"/>
    </Grid>
</Window>
